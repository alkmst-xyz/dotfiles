{
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Dracula",
  "window.menuBarVisibility": "toggle",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.rulers": [79],
  "editor.detectIndentation": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 16,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.languageServer": "Jedi",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "clangd.arguments": ["--compile-commands-dir=build/debug"],
  "vim.useCtrlKeys": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "docker.dockerPath": "podman",
  "remote.containers.dockerPath": "podman",
  "mesonbuild.buildFolder": "build",
  "mesonbuild.configureOnOpen": false,
  "[nunjucks]": {
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter"
  },
  "files.associations": {
    "*.html": "html"
  },
  "[latex]": {
    "editor.defaultFormatter": "nickfode.latex-formatter"
  }
}
